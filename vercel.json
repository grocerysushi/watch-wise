{
  "rewrites": [
    {
      "source": "/movie/:id/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/tv/:id/:slug",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}